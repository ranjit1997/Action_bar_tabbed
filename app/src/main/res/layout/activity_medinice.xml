<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".medinice">

    <RelativeLayout
        android:backgroundTint="@color/white"
        android:elevation="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginLeft="5dp"
        android:layout_width="match_parent"
        android:background="@drawable/item_gridview"
        android:layout_height="160dp">

        <ImageView
            android:id="@+id/os_images"
            android:layout_marginTop="20dp"
            android:layout_alignParentLeft="true"
            android:layout_width="120dp"
            android:layout_marginLeft="10dp"
            android:layout_height="120dp"
            android:src="@drawable/cough_medicine" />

        <TextView
            android:id="@+id/os_texts"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:text="Benadryl Cough Medicine"
            android:layout_marginLeft="140dp"
            android:layout_marginTop="40dp"
            android:textSize="14dp"
            android:textStyle="bold"
            tools:ignore="HardcodedText,SpUsage" />
        <TextView
            android:id="@+id/os_texts1"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:text="This is use for cough Read more..."
            android:layout_marginLeft="140dp"
            android:layout_marginTop="75dp"
            android:textSize="14dp"
            tools:ignore="HardcodedText,SpUsage" />

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="160dp"
        android:layout_height="50dp"
        android:layout_marginTop="2dp"
        android:layout_marginLeft="17dp"
        android:layout_marginBottom="5dp"
        android:elevation="10dp"
        android:alpha="0.98"
        android:layout_alignParentBottom="true"
        android:background="@drawable/background"
        android:id="@+id/relLayout2">

        <!--<ImageView-->
            <!--android:id="@+id/ic_magnify2"-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_marginLeft="20dp"-->
            <!--android:layout_centerVertical="true"-->
            <!--android:src="@drawable/email" />-->
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="55dp"
            android:textSize="15dp"
            android:textColor="@color/white"
            android:text="FEEDBACK"
            android:singleLine="true"
            tools:ignore="NotSibling,UnknownId" />


    </RelativeLayout>

    <RelativeLayout
        android:layout_width="160dp"
        android:layout_height="50dp"
        android:layout_marginTop="2dp"
        android:layout_marginLeft="185dp"
        android:layout_marginBottom="5dp"
        android:elevation="10dp"
        android:alpha="0.94"
        android:layout_alignParentBottom="true"
        android:background="@drawable/background"
        android:id="@+id/relLayout3">

        <!--<ImageView-->
            <!--android:id="@+id/ic_magnify3"-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_marginLeft="20dp"-->
            <!--android:src="@drawable/phoneim"-->
            <!--android:layout_centerVertical="true" />-->
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="55dp"
            android:textSize="15dp"
            android:textColor="@color/white"
            android:text="LOCATION"
            android:singleLine="true"
            tools:ignore="NotSibling,UnknownId" />

    </RelativeLayout>

</RelativeLayout>